{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "cSpell.words": [
    "Cmro",
    "colorpicker",
    "colspan",
    "columnheader",
    "columnsorter",
    "combobox",
    "datetimepicker",
    "domcontentloaded",
    "endtests",
    "FEDSPRING",
    "filechooser",
    "FULLNAME",
    "gridcell",
    "iife",
    "jsxdev",
    "likert",
    "listbox",
    "mailparser",
    "navs",
    "networkidle",
    "numerictextbox",
    "onspringend",
    "optionlabel",
    "pwservice",
    "signin",
    "tabstrip",
    "teardowns",
    "textbox",
    "timespan",
    "treeitem",
    "Treeview",
    "Unlaunched",
    "VNEXT",
    "VPRIOR"
  ],
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "githubIssues.issueBranchTitle": "${user}/issue-${issueNumber}-${sanitizedIssueTitle}",
  "playwright.workspaceSettings": {
    "configs": [
      {
        "relativeConfigFile": "playwright.config.ts",
        "selected": true,
        "enabled": true,
        "projects": [
          {
            "name": "chrome",
            "enabled": true
          },
          {
            "name": "edge",
            "enabled": false
          },
          {
            "name": "cleanup",
            "enabled": false
          },
          {
            "name": "setup",
            "enabled": true
          },
          {
            "name": "teardown",
            "enabled": true
          }
        ]
      },
      {
        "relativeConfigFile": "playwright.service.config.ts",
        "selected": false,
        "enabled": false,
        "projects": []
      }
    ]
  }
}
